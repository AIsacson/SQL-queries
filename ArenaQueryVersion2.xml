let $arena := (doc("project.xml")/Turneringar/Turnering/Arena)
return
<Resultat>{
for $namn in distinct-values($arena/@namn)
for $stad in distinct-values($arena[@namn = $namn]/@stad)
order by $namn
return <Arena stad="{$stad}" namn="{$namn}"/>
}
</Resultat>

